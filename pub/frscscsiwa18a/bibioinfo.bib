journal = {Metabolomics},
keywords = {Acidithiobacillus,Bioleaching,Biomarker,CE-MS,Capillary electrophoresis,Ferrooxidans,Licanantay,Mass spectrometry,Metabolomics,Spermidine,Thiooxidans,Wenelen},
