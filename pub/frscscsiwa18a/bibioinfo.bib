file = {:Users/cfrioux/biblio/taffs2009.pdf:pdf},
issn = {1752-0509 1752-0509},
journal = {BMC systems biology},
keywords = {*Ecosystem,*Models- Biological,Chloroflexus/*physiology,Circadian Rhythm/physiology,Energy Metabolism/*physiology,Metabolic Networks and Pathways/*physiology,Nitrogen Fixation/physiology,Species Specificity,Synechococcus/*physiology,Wyoming},
