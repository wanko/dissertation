isbn = {8756-7938},
issn = {87567938},
keywords = {Flux balance analysis,Multiobjective linear programming,Noninferior set estimation method,flux balance analysis,multiobjective linear programming,noninferior set estimation method},
language = {en},
mendeley-tags = {flux balance analysis,multiobjective linear programming,noninferior set estimation method},
number = {4},
