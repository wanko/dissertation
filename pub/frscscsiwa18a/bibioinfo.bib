isbn = {1553-7358},
issn = {1553734X},
journal = {PLoS Computational Biology},
month = {jun},
number = {6},
pages = {e1003081},
pmid = {23843751},
publisher = {Public Library of Science},
title = {{Flux Balance Analysis of Cyanobacterial Metabolism: The Metabolic Network of Synechocystis sp. PCC 6803}},
