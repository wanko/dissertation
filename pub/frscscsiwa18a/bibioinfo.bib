isbn = {1553-7358 (Electronic)$\backslash$n1553-734X (Linking)},
issn = {1553734X},
journal = {PLoS Computational Biology},
keywords = {*Ecosystem,*Microbial Interactions,*Models- Biological,Algorithms,Culture Media/chemistry/metabolism,Desulfovibrio vulgaris/metabolism,Escherichia coli/metabolism,Methanococcus/metabolism,Saccharomyces cerevisiae/metabolism,Synthetic Biology/*methods,Systems Biology/*methods},
language = {eng},
mendeley-tags = {*Ecosystem,*Microbial Interactions,*Models- Biological,Algorithms,Culture Media/chemistry/metabolism,Desulfovibrio vulgaris/metabolism,Escherichia coli/metabolism,Methanococcus/metabolism,Saccharomyces cerevisiae/metabolism,Synthetic Biology/*methods,Systems Biology/*methods},
