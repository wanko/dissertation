journal = {PloS One},
keywords = {Actinobacteria,Algorithms,Biomass,Computational Biology,Computers,Gene Expression Regulation- Bacterial,Genome,Genome- Bacterial,Metabolic Networks and Pathways,Models- Biological,Software,Species Specificity,Streptomyces},
language = {eng},
mendeley-tags = {Actinobacteria,Algorithms,Biomass,Computational Biology,Computers,Gene Expression Regulation- Bacterial,Genome,Genome- Bacterial,Metabolic Networks and Pathways,Models- Biological,Software,Species Specificity,Streptomyces},
number = {12},
