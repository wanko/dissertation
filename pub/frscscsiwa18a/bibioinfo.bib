@article{Hosseini2015,
abstract = {Flux coupling analysis is a method for investigating the connections between reactions of metabolic networks. Here, we construct the hierarchical flux coupling graph for the reactions of the Escherichia coli metabolic network model to determine the level of each reaction in the graph. This graph is constructed based on flux coupling analysis of metabolic network: if zero flux through reaction a results in zero flux through reaction b (and not vice versa), then reaction a is located at the top of reaction b in the flux coupling graph. We show that in general, more important, older and essential reactions are located at the top of the graph. Strikingly, genes corresponding to these reactions are found to be the genes which are most regulated.},
author = {Hosseini, Zhaleh and Marashi, Sayed-Amir},
doi = {10.1016/j.gene.2015.02.032},
