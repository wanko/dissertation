journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {core and pan genome,mathematical modeling,systems biology},
month = {dec},
number = {50},
pages = {20338--43},
pmid = {24277855},
publisher = {National Academy of Sciences},
title = {{Genome-scale metabolic reconstructions of multiple Escherichia coli strains highlight strain-specific adaptations to nutritional environments.}},
