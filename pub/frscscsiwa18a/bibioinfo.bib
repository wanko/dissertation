month = {oct},
number = {3},
pages = {403--412},
publisher = {Wiley Subscription Services, Inc., A Wiley Company},
title = {{Systematizing the generation of missing metabolic knowledge}},
url = {http://doi.wiley.com/10.1002/bit.22844},
volume = {107},
year = {2010}
}
