@article{Orth2010a,
abstract = {Genome-scale metabolic network reconstruc- tions are built from all of the known metabolic reactions and genes in a target organism. However, since our knowledge of any organism is incomplete, these network reconstructions contain gaps. Reactions may be missing, resulting in dead- ends in pathways, while unknown gene products may cat- alyze known reactions. New computational methods that analyze data, such as growth phenotypes or gene essentiality, in the context of genome-scale metabolic networks, have been developed to predict these missing reactions or genes likely to fill these knowledge gaps. A growing number of experimental studies are appearing that address these com- putational predictions, leading to discovery of new meta- bolic capabilities in the target organism. Gap-filling methods can thus be used to improve metabolic network models while simultaneously leading to discovery of new metabolic gene functions.},
author = {Orth, Jeffrey D. and Palsson, Bernhard {\O}.},
doi = {10.1002/bit.22844},
file = {:Users/cfrioux/biblio/Orth{\_}et{\_}al-2010-Biotechnology{\_}and{\_}Bioengineering.pdf:pdf},
issn = {00063592},
journal = {Biotechnology and Bioengineering},
