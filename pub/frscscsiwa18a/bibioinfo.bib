mendeley-tags = {*Models- Theoretical,Coculture Techniques,Desulfovibrio vulgaris/growth {\&} development/*metab,Electrons,Hydrogen/metabolism,Methanococcus/growth {\&} development/*metabolism,Species Specificity,Sulfates/metabolism},
pages = {92},
title = {{Metabolic modeling of a mutualistic microbial community.}},
